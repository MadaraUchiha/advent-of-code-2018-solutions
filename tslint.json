{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark": [
            true,
            "single"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}